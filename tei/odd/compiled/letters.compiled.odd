                                   <assert test="doc-available(concat($document_uri,'/',@target))">ERROR: <name/> points to a non-existent file (<value-of select="@target"/>)</assert>
                               </rule>
                           </constraint>
                       </constraintSpec><constraintSpec scheme="schematron" ident="refAtts" xml:lang="en">
    <constraint>
