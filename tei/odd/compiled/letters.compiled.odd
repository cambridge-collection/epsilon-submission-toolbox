    <egXML xmlns="http://www.tei-c.org/ns/Examples" xml:id="gi-ex-egXML-ma">The address is Southmoor <choice><expan>R<ex>oa</ex>d</expan><abbr>Rd</abbr></choice>
      </egXML>
